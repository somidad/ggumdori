{
  "name": "ggumdori",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "supabase-types": "dotenv -e .env -- bash -c 'npx --yes supabase gen types --project-id $EXPO_PUBLIC_SUPABASE_PROJECT_ID > utils/supabase-types.ts'",
    "license": "npx license-report --only=prod --output=csv --csvHeaders --delimiter=\"\t\" --fields=name --fields=licenseType --fields=link --fields=remoteVersion --fields=installedVersion --fields=author > license.tsv",
    "prebuild": "APP_VARIANT=development expo prebuild",
    "prebuild:release": "APP_VARIANT=release expo prebuild",
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "android:release": "APP_VARIANT=release expo run:android --variant release",
    "ios": "expo run:ios",
    "ios:release": "APP_VARIANT=release expo run:ios --variant release",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "^2.1.1",
    "@react-native-google-signin/google-signin": "^13.1.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.48.1",
    "expo": "~52.0.33",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.5",
    "expo-font": "~13.0.3",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "~0.29.21",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-paper": "^5.13.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/metro-config": "^0.19.9",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "dotenv-cli": "^8.0.0",
    "expo-dev-client": "~5.0.12",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.3",
    "license-report": "^6.7.1",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
